    <template id="website_task.qwebtemplate" name="Snippet Template">
        <div class="emp-data-1">
            <h2>List of Items:</h2>
            <ul>
                <t t-foreach="items" t-as="item">
                    <li><t t-esc="item.ename" /></li>
                </t>
            </ul>
        </div>
    </template>
    
